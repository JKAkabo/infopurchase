                    },
                    {
                        "name": "itemId",
                        "in": "path",
                        "description": " Order Item No a parameter",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "ItemID": {
                                        "type": "string"
                                    },
                                    "UnitCost": {
                                        "type": "string"
                                    },
                                    "StockLevel": {
                                        "type": "double"
                                    },
                                    "ExpiryDate": {
                                        "type": "DateTime"
                                    },
                                    "BatchNo": {
                                        "type": "string"
                                    },
                                    "StoreID": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "ItemID": "D22231",
                                    "UnitCost": "1392813",
                                    "StockLevel": "223",
                                    "ExpiryDate": "2020-01-31 00:00:00.000",
                                    "BatchNo": "237372",
                                    "StoreID": "ST3372"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
