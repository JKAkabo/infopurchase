        "/api/v1/pending-receival/{PONo}": {
            "post": {
                "tags": [
                    "Purchase Order"
                ],
                "summary": "Generate a new Purchase Orders Pending Receival",
                "operationId": "App\\Http\\Controllers\\ApiController::updateApprovalPendingReceival",
                "parameters": [
                    {
                        "name": "PONo",
                        "in": "path",
                        "description": "Provider Purchase Order No a parameter",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "PONo": {
                                        "type": "string"
                                    },
                                    "ItemID": {
                                        "type": "string"
                                    },
                                    "UnitCost": {
                                        "type": "string"
                                    },
                                    "StockLevel": {
                                        "type": "double"
                                    },
                                    "ExpiryDate": {
                                        "type": "DateTime"
                                    },
                                    "BatchNo": {
                                        "type": "string"
                                    },
                                    "StoreID": {
                                        "type": "string"
                                    },
                                    "OrderID": {
                                        "type": "string"
                                    },
                                    "SupplierID": {
                                        "type": "string"
                                    },
                                    "OrderUnit": {
                                        "type": "string"
                                    },
                                    "ItemQuantity": {
                                        "type": "string"
                                    },
                                    "QuantityOrdered": {
                                        "type": "string"
                                    }
                                },
                                "type": "object",
                                "example": {
                                    "PONo": "03592018",
                                    "ItemID": "D22231",
                                    "UnitCost": "1392813",
                                    "StockLevel": "223",
                                    "ExpiryDate": "2020-01-31 00:00:00.000",
                                    "BatchNo": "237372",
                                    "StoreID": "ST3372",
                                    "OrderID": "234543",
                                    "Description": "Table of Bla bla",
                                    "SupplierID": "3244",
                                    "OrderUnit": "TABLETS",
                                    "ItemQuantity": "2000.78",
                                    "QuantityOrdered": "3994.89",
                                    "OrderLineID": "1",
                                    "OrderUnitID": 1
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/v1/save-stored/{PONo}/{itemId}": {
            "post": {
                "tags": [
